    "@types/react-router-dom": "^5.3.3",
    "@types/react-vis": "^1.11.11",
    "ajv": "^8.12.0",
    "axios": "^1.3.6",
    "bootstrap": "^5.2.3",
    "check-password-strength": "^2.0.7",
    "client-zip": "^2.3.1",
